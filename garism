cli.Activate()
getLastRow := cli.Cells(cli.Rows.Count, 1).End(-4162).Row  ; xlUp

for row := 2, row <= getLastRow, row++ {
    colGValue := cli.Cells(row, 7).Value   ; Column G
    AddToLog("Row " row " → G: " colGValue)
}


cli.Activate()
getLastRow := cli.Cells(cli.Rows.Count, 1).End(-4162).Row  ; xlUp

; correct syntax → note the parentheses
for row in (2 .. getLastRow) {
    colGValue := cli.Cells(row, 7).Value  ; column G
    AddToLog("Row " row " → G: " colGValue)
}
