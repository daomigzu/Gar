Sub btnPlanCheck()
    Dim wsSource As Worksheet
    Dim wsTarget As Worksheet
    Dim valueToCopy As Variant

    '=== Set sheet references ===
    Set wsSource = ThisWorkbook.Sheets("Claim Line Item")
    Set wsTarget = ThisWorkbook.Sheets("Plan Checker")

    '=== Get the value from A2 in Claim Line Item ===
    valueToCopy = wsSource.Range("A2").Value

    '=== Paste it into J2 in Plan Checker ===
    wsTarget.Range("J2").Value = valueToCopy

    '=== Optional confirmation ===
    MsgBox "âœ… Copied value from 'Claim Line Item'!A2 to 'Plan Checker'!J2" & vbCrLf & _
           "Value: " & valueToCopy, vbInformation, "btnPlanCheck"
End Sub
