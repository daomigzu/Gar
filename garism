Sub checkForBH()
    Dim ws As Worksheet
    Dim lastRow As Long
    Dim i As Long
    Dim foundBH As Boolean
    Dim cellValue As String

    '=== Set target sheet ===
    Set ws = ThisWorkbook.Sheets("Plan Checker")

    '=== Find the last used row in column A ===
    On Error Resume Next
    lastRow = ws.Cells(ws.Rows.Count, "A").End(xlUp).Row
    On Error GoTo 0

    '=== Default: no BH claim ===
    foundBH = False

    '=== Loop through rows starting at 2 ===
    For i = 2 To lastRow
        cellValue = Trim(ws.Cells(i, "A").Value)
        If Len(cellValue) >= 3 Then
            If UCase(Left(cellValue, 3)) = "CBH" Then
                foundBH = True
                Exit For
            End If
        End If
    Next i

    '=== Output result ===
    If foundBH Then
        ws.Range("K14").Value = "BH CLAIM"
    Else
        ws.Range("K14").ClearContents
    End If
End Sub
